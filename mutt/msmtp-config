passwordeval "gpg2 --use-agent -d /home/mario/.config/mutt/unimi.gpg"
auth on
tls on
tls_starttls off
tls_trust_file	/etc/ssl/cert.pem
logfile /home/mario/.config/msmtp/msmtp.log

